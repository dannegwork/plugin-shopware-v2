<?xml version="1.0" encoding="utf-8"?>
<cronjobs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/shopware/shopware/5.3/engine/Shopware/Components/Plugin/schema/cronjob.xsd">

    <cronjob>
        <name>Boxalino Export</name>
        <action>Shopware_CronJob_BoxalinoExportCron</action>
        <active>true</active>
        <interval>86400</interval>
        <disableOnError>true</disableOnError>
    </cronjob>

    <cronjob>
        <name>Boxalino Export Delta</name>
        <action>Shopware_CronJob_BoxalinoExportCronDelta</action>
        <active>true</active>
        <interval>3600</interval>
        <disableOnError>true</disableOnError>
    </cronjob>

</cronjobs>